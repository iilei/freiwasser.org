# Example terraform.tfvars file
# Copy this to terraform.tfvars and fill in your values

# Domain configuration
domain = "freiwasser.org"

# Subdomains to configure
subdomains = ["api", "auth", "tiles", "www"]

# SSH public key path for server access
ssh_public_key_path = "~/.ssh/id_ed25519_hetzner_20251121.pub"

# Reverse proxy choice: "nginx" or "traefik"
reverse_proxy = "nginx"
